    "mongodb": "^5.1.0",
    "nestjs-real-ip": "^2.2.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "playwright": "^1.31.2",
    "rate-limiter-flexible": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@automock/jest": "^1.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
